{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  extends: [
    "config:base",
    "group:allNonMajor",
    "schedule:monthly",
    ":automergeMinor",
  ],
  dependencyDashboardAutoclose: true,
  dependencyDashboardHeader: "\
This issue lists Renovate updates and detected dependencies. See the Renovate \
[docs](https://docs.renovatebot.com/key-concepts/dashboard/) and/or the \
[job log](https://app.renovatebot.com/dashboard#github/CutieCity/guide) for \
more information.",
  enabledManagers: ["github-actions", "pip_requirements"],
  pip_requirements: {
    fileMatch: ["^requirements\\.in$"],
    ignorePaths: ["requirements.txt"], // Autogenerated by pip-compile.
  },
  automergeType: "branch",
  ignoreTests: true,
}
